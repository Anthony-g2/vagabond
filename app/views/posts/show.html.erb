<h2><%= @post.title %></h2>
<% @city = City.find_by_id(@post.city_id) %>
<small>For <%= link_to "#{@city.name}", city_path(@city) %></small>
<p><%= @post.message %></p>
<% if session[:user_id] == nil %>
  <p><%= @user.name %></p>
<% else %>
  <%= link_to "#{@user.name}", user_path(@user) %>
<% end %>
