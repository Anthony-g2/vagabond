<div class="page_body container">
  <h1>Create a New Post</h1>
  <% flash.each do |name, msg| %>
   <p>
     <%= name.capitalize %>: <%= msg %>
   </p>
   <% end %>
   <%= render 'form', post: @post %>
  <br>
  <% @city = City.find_by_id(session[:last_city_id]) %>
  <%= link_to material_icon.arrow_back, city_path(@city) %>
</div>
